@model List<ServiceItem>
@{
    TextField textField = ViewBag.TextField;
    ViewBag.Title = textField.Title;
}
<div>
    @if (Model.Any())
    {
        <ul class="big-image-list">
            @foreach (ServiceItem entity in Model)
            {
                <li>
                    <a asp-area="" asp-controller="Services" asp-action="Index" asp-route-id="@entity.Id">
                        <img src="~/images/service/@entity.TitleImagePath" alt="" class="left" />
                    </a>
                    <h4>@entity.Title</h4>
                    <p>@entity.Subtitle</p>
                </li>
            }
        </ul>
        <div style="text-align:center">
            @Html.Raw(textField.Text)
        </div>
    }
    else
    {
        <p>Услуги найдены</p>
    }
</div>